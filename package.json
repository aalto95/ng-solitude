{
  "name": "ng-solitude",
  "version": "0.0.1",
  "scripts": {
    "storybook": "ng run ng-solitude:storybook",
    "build-storybook": "ng run ng-solitude:build-storybook",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-solitude --configuration production && npm run build:styles",
    "build:styles": "sass --no-source-map src/styles/styles.scss dist/ng-solitude/styles.css",
    "build:publish": "npm run build && npm run publish",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "publish": "cd ./dist/ng-solitude/ && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaltoo/ng-solitude.git"
  },
  "peerDependencies": {
    "@angular/animations": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/common": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/compiler": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/core": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/forms": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/platform-browser": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/router": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.0 || ^0.12.0 || ^0.13.0"
  },
  "devDependencies": {
    "@angular/animations": "14.3.0",
    "@angular/common": "14.3.0",
    "@angular/compiler": "14.3.0",
    "@angular/core": "14.3.0",
    "@angular/forms": "14.3.0",
    "@angular/platform-browser": "14.3.0",
    "@angular/platform-browser-dynamic": "14.3.0",
    "@angular/router": "14.3.0",
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-backgrounds": "^7.0.21",
    "@storybook/addon-controls": "^7.0.21",
    "@storybook/addon-docs": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^7.0.21",
    "@storybook/addons": "^7.0.21",
    "@storybook/angular": "^7.0.21",
    "@storybook/theming": "^7.0.21",
    "@types/jasmine": "~3.10.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
